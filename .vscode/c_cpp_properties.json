{
    "configurations": [
        {
            "name": "STM32 L4",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "USE_HAL_DRIVER",
                "STM32L476xx",
                "ARM_MATH_CM4",
                "__FPU_PRESENT"
            ],
            "windowsSdkVersion": "10.0.19041.0",
            "compilerPath": "${env:ARMGCC_DIR}/arm-none-eabi-gcc.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "configurationProvider": "ms-vscode.makefile-tools",
            "compilerArgs": ["-mcpu=cortex-m4", "-mthumb"]
        }
    ],
    "version": 4
}